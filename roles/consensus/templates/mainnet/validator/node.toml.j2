#########################################################
# Node-specific configuration
#########################################################
beneficiary = "0x32794222b3b5d48801ac40ecf9b9a2847c713351"
node_name = "{{ org }}" # node name
network_name = "mainnet" # DO NOT CHANGE
ipc_tx_batch_size = 1000
ipc_max_queued_batches = 10
# must be <= ipc_max_queued_batches
ipc_queued_batches_watermark = 3

statesync_threshold = 600
statesync_max_concurrent_requests = 5

#########################################################
# Network-wide configuration
#########################################################
chain_id = 143

[peer_discovery]
self_address = "134.119.182.147:8000" # IP and port
self_record_seq_num = 1 # should be 0 initially
self_auth_port = 8001
self_name_record_sig = "f1db41cf2ae6d961e11f1dcc2e64cf6f83cf5d4f1c676f686ed5ac9045c2549f27025df0590fb75427dc509de9370c1df43741be71c7b75bc67d36d961e603f001"
